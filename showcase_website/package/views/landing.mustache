<!doctype html>
<html lang="en">
<head>
    <title>eGrow</title>
    <meta name="description" content="" />

    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="{{{page.img_favicon}}}=s32-c" type="image/png" rel="icon" />
    <link href="{{{cdn}}}/dist/css/style.min.css" rel="stylesheet">

</head>

<body>
<div class="blocks-page">
    <!--navigation-->
    <section class="smart-scroll">
        <div class="container-fluid">
            <nav class="navbar navbar-expand-md navbar-light">
                <a class="navbar-brand heading-black" href="/">
                    <img class="img-logo" src="{{{page.img_logo_nav}}}=s60"/>
                </a>
                <button class="navbar-toggler navbar-toggler-right border-0" type="button" data-toggle="collapse"
                        data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false"
                        aria-label="Toggle navigation">
                    <i class="fa fa-bars"></i>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <ul class="navbar-nav ml-auto mobile-nav">
                        {{#page.header_nav.nav1.show_or_hide}}
                            <li  class="nav-item">
                                <a style="color: #ffffff; background-color: #336666; border-radius: 10%; opacity: 0.8" class="nav-link" href="#decouvrir">{{page.header_nav.nav1.text}}</a>
                            </li>
                        {{/page.header_nav.nav1.show_or_hide}}

                        {{#page.header_nav.nav2.show_or_hide}}
                            <li class="nav-item">
                                <a style="color: #ffffff; background-color: #336666; border-radius: 10%;opacity: 0.8" class="nav-link" href="#rse">{{page.header_nav.nav2.text}}</a>
                            </li>
                        {{/page.header_nav.nav2.show_or_hide}}

                        {{#page.header_nav.nav3.show_or_hide}}
                            <li class="nav-item">
                                <a style="color: #ffffff; background-color: #336666; border-radius: 10%;opacity: 0.8" class="nav-link" href="#lien">{{page.header_nav.nav3.text}}</a>
                            </li>
                        {{/page.header_nav.nav3.show_or_hide}}

                        {{#page.header_nav.nav4.show_or_hide}}
                            <li class="nav-item">
                                <a style="color: #ffffff; background-color: #336666; border-radius: 10%;opacity: 0.8" class="nav-link" href="#membres">{{page.header_nav.nav4.text}}</a>
                            </li>
                        {{/page.header_nav.nav4.show_or_hide}}

                        {{#page.header_nav.nav5.show_or_hide}}
                            <li class="nav-item">
                                <a style="color: #ffffff; background-color: #336666; border-radius: 10%;opacity: 0.8" class="nav-link" href="#contact">{{page.header_nav.nav5.text}}</a>
                            </li>
                        {{/page.header_nav.nav5.show_or_hide}}
                    </ul>
                </div>
            </nav>
        </div>
    </section>
    <!-- INTRODUCTION-->
    <section style="background:url('{{{page.introduction.img_background}}}=s2048');background-size:cover;background-position:center;">
        <div class="gradient-div py-3" style="height:76vh">

            <div class="container">
                <div class="row justify-content-center header-row">
                    <div class="col-12 col-md-8 col-lg-7 text-center text-white">
                        <img class="mt-4" style="width:200px;border-radius:50%" src="{{{page.introduction.img_logo}}}=s200-c"/>
                        <h1 class="display-3 mt-0">{{page.introduction.header}}</h1>
                        <p class="lead text-white mb-4 mt-0">
                          {{page.introduction.subheader}}
                        </p>
                        <a href="#decouvrir" class="btn btn-info btn-lg" style="background-color: #336666; border-color: #336666 ">Nous découvrir</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Nous Découvrir -->
    <section id="decouvrir" class="py-md-5 block">
        <div class="container py-5">
            <div class="row block-header">
                <div class="col-sm-8 mx-auto text-center">
                    <h1 class="display-4 mt-2 section-title">{{page.features.feature-4.header}}</h1>
                    <hr>
                    <p class="lead">{{page.features.feature-4.subheader1}}</p>
                    <p class="lead">{{page.features.feature-4.subheader2}}</p>
                    <p class="lead">{{page.features.feature-4.subheader3}}</p>
                    <p class="lead">{{page.features.feature-4.subheader4}}</p>
                </div>
            </div>
            <div class="row mt-5 align-items-center mb-5">
                <div class="col-md-6 ml-0">
                    <h4>{{page.features.feature-4.article1Header}}</h4>
                    <p>{{page.features.feature-4.article1Text1}}</p>
                    <p>{{page.features.feature-4.article1Text2}}</p>
                </div>
                <div class="col-md-5 order-md-first">
                    <img style="border-radius: 10%" class="rounded-corner" src="{{{page.features.feature-4.img_1}}}" alt="...">
                </div>
            </div>
            <div class="row align-items-center mt-5">
                <div class="col-md-6 mr-0">
                    <h4>{{page.features.feature-4.article2Header}}</h4>
                    <p>{{page.features.feature-4.article2Text}}</p>
                </div>
                <div class="col-md-5">
                    <img style="border-radius: 10%" class="rounded-corner" src="{{{page.features.feature-4.img_2}}}" alt="...">
                </div>
            </div>
        </div>
    </section>

    <!--RSE--->
    <section id="rse" class="py-md-5" style="background:#f9f9f9">
        <div class="container py-5">
            <div class="row justify-content-center text-center section-intro">
                <div class="col-12 col-md-9 col-lg-8">
                    <h1 class="display-4 ">{{page.projects.header}}</h1>
                    <p class="lead">{{page.projects.subheader}}</p>
                </div>
            </div>

            <ul class="feature-list feature-list-lg">
                <li class="row align-items-center mb-4">
                    <div class="col-12 col-md-6 col-lg-3 offset-lg-2">
                        <img class="img-fluid rounded" alt="Image" src="{{{page.projects.img_1}}}=s1024-c" />
                    </div>

                    <div class="col-12 col-md-6 col-lg-6">
                        <h5>{{page.projects.header1}}</h5>
                        <p>
                          {{page.projects.texte1}}
                        </p>
                        <p>
                            {{page.projects.texte11}}
                        </p>
                    </div>
                </li>

                <li class="row align-items-center mb-4">
                    <div class="col-12 col-md-6 col-lg-3 offset-lg-2">
                        <img class="img-fluid rounded" alt="Image" src="{{{page.projects.img_2}}}=s1024-c" />
                    </div>
                    <!--end of col-->
                    <div class="col-12 col-md-6 col-lg-6">
                        <h5>{{page.projects.header2}}</h5>
                        <p>
                            {{page.projects.texte2}}
                        </p>
                        <p>
                            {{page.projects.texte22}}
                        </p>
                    </div>
                </li>

                <li class="row align-items-center mb-4">
                    <div class="col-12 col-md-6 col-lg-3 offset-lg-2">
                        <img class="img-fluid rounded" alt="Image" src="{{{page.projects.img_3}}}=s1024-c" />
                    </div>
                    <div class="col-12 col-md-6 col-lg-6">
                        <h5>{{page.projects.header3}}</h5>
                        <p>
                            {{page.projects.texte3}}
                        </p>
                        <p>
                            {{page.projects.texte33}}
                        </p>
                    </div>
                </li>
            </ul>
        </div>
    </section>

    <!-- Telecharger Application -->
    <style>
        .container {
            position: relative;
            width: 50%;
        }

        .image {
            opacity: 1;
            display: block;
            width: 100%;
            height: auto;
            transition: .5s ease;
            backface-visibility: hidden;
        }

        .middle {
            transition: .5s ease;
            opacity: 0;
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%)
        }

        .container:hover .image {
            opacity: 0.3;
        }

        .container:hover .middle {
            opacity: 1;
        }

        .text {
            background-color: #4CAF50;
            color: white;
            font-size: 16px;
            padding: 16px 32px;
        }
    </style>
    <section id="lien" class="py-5 pricing-block" style="background:#f9f9f9">
        <div class="container py-5">
            <div class="row block-header">
                <div class="col-sm-8 mx-auto text-center">
                    <h1 class="display-4 mt-2 section-title">{{page.app.header}}</h1>
                    <hr>
                    <p class="lead">{{page.app.text}}</p>
                </div>
            </div>
            <div class="text-center">
                <div style="display: block; margin-left: auto; margin-right: auto">
                    <div class="pricing active">
                        <p style="padding: 1rem;font-size: 1rem;font-weight: 600;border-bottom: 1px solid #f1f2f3;background-color: #fcfdfe;">{{page.app.hLink}}</p>
                        <div style="margin-bottom: 20px" class="container">
                            <img src="{{page.app.image}}" alt="Avatar" class="image" style="width:100%">
                            <div class="middle">
                                <div><a class="text" href="{{page.app.link}}">{{page.app.download}}</a></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!---Membres du projet--->
    <section id="membres" class="py-md-5 block testimonial-block">
        <div class="container py-5">
            <div class="row block-header">
                <div class="col-sm-8 mx-auto text-center">
                    <h1 class="display-4 section-title">{{page.groupe.header}}</h1>
                    <hr>
                    <p class="lead">{{page.groupe.subheader}}</p>
                </div>
            </div>
                <!--Robin-->
            <div class="row mt-5 text-center">
                <div class="col-md-6 mb-5">
                    <blockquote class="blockquote">
                        <p><img class="avatar" src="{{{page.groupe.robinImg}}}" alt="..."></p>
                        <p>{{page.groupe.robinText}}</p>
                        <p class="name">Robin Castermane {Scrum Master}</p>
                    </blockquote>
                </div>
            <!--Louis-->
            <div class="col-md-6 mb-5">
                <blockquote class="blockquote">
                    <p><img class="avatar" src="{{{page.groupe.louisImg}}}" alt="..."></p>
                    <p>{{page.groupe.louisText}}</p>
                    <p class="name">Louis Brochet {Team Member}</p>
                </blockquote>
            </div>
                    <!--rodrigue-->
            <div class="col-md-6 mb-5">
                <blockquote class="blockquote">
                    <p><img class="avatar" src="{{{page.groupe.rodrigueImg}}}" alt="..."></p>
                    <p>{{page.groupe.rodrigueText}}</p>
                    <p class="name">Rodrigue Mugisha {Team Member} </p>
                </blockquote>
            </div>
            <!--Igor-->
            <div class="col-md-6 mb-5" >
                <blockquote class="blockquote">
                    <p><img class="avatar" src="{{{page.groupe.igorImg}}}" alt="..."></p>
                    <p>{{page.groupe.igorText}}</p>
                    <p class="name">Igor Vandervelden {Product Owner}</p>
                </blockquote>
            </div>
            <!--Victor-->
            <div class="col-md-6 mb-5">
                <blockquote class="blockquote">
                    <p><img class="avatar" src="{{{page.groupe.victorImg}}}" alt="..."></p>
                    <p>{{page.groupe.victorText}}</p>
                    <p class="name">Victor Cotton {Team Member}</p>
                </blockquote>
            </div>
            <!--Leon-->
            <div class="col-md-6 mb-5">
                <blockquote class="blockquote">
                    <p><img class="avatar" src="{{{page.groupe.leonImg}}}" alt="..."></p>
                    <p>{{page.groupe.leonText}}</p>
                    <p class="name">Léon Servais {Team Member}</p>
                </blockquote>
            </div>

                        <!--Victoire-->
            <div style="margin: auto" class="col-md-6 mb-5">
                <blockquote class="blockquote">
                    <p><img class="avatar" src="{{{page.groupe.victoireImg}}}" alt="..."></p>
                    <p>{{page.groupe.victoireText}}</p>
                    <p class="name">Victoire Michel {Scrum Master}</p>
                </blockquote>
            </div>
        </div>
        </div>
    </section>

    <!---Contact--->
    <section id="contact" class="py-md-5 mb-2r">
        <div class="container">
            <div class="row">
                <div class="col-sm-8 mx-auto text-center">
                    <h1 class="display-4 mt-2 section-title">{{page.contact.header}}</h1>
                    <p>{{page.contact.subheader}}</p>
                </div>
            </div>
            <div id="contact" class="row mt-5">
                <div class="col-sm-6 mx-auto" data-aos="fade-left" data-aos-duration="1000">
                    <form action="" method="post">
                        <div class="form-group">
                            <input type="text" id="contact-form-name" class="form-control" placeholder="Nom">
                        </div>
                        <div class="form-group">
                            <input type="text" id="contact-form-email" class="form-control" placeholder="Email">
                        </div>
                        <div class="form-group">
                            <input type="text" id="contact-form-subject" class="form-control" placeholder="Sujet">
                        </div>
                        <div class="form-group">
                            <textarea name="" id="contact-form-message" cols="30" rows="7" class="form-control" placeholder="Message"></textarea>
                        </div>
                        <div class="form-group text-center">
                            <button id="btn-contact-submit" class="btn btn-block btn-info">Envoyer Message</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>

<!--FOOTER-->
    <footer class="pt-3">
        <div class="container pt-5 pb-5">
            <div class="row">
                <div class="col-md-12 text-center">
                    <p  style="font-size: 20px; color: #ffffff">{{page.footer.text}}</p>
                    <p><a target="_blank" href="{{page.footer.githubLink}}"><img class="img-logo-footer" src="{{{page.img_logo_footer}}}=s200"/></a></p>
                </div>
            </div>
        </div>
    </footer>
</div>

{{>scripts}}
<script src="{{{cdn}}}/js/contact.js"></script>
</body>
</html>
